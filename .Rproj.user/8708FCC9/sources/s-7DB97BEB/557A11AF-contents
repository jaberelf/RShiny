x <- rnorm(50)
y <- x + rnorm(50, mean = 50)
cor(x,y)
plot(x,y)

x <- 1:10
y<- 1:10
f <- outer(x,y,function (x,y)cos(y)/(1+x^2)) 
contour (x,y,f)  
contour (x,y,f,nlevels =45,add=T)  
fa=(f-t(f))/2 
contour (x,y,fa,nlevels =15)

library(ISLR)
auto <- ISLR::Auto
class(auto)
colnames(auto)
names(auto)
names(auto) == colnames(auto)

attach(auto)
plot(year, horsepower, type = "l")



plot(horsepower ,mpg) 
identify(mpg, horsepower, cylinders)

summary(auto)
